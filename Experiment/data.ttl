qmd:throughput
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Throughput of the sources measures the amount of HTTP requests answered per second."@en ;
  skos:prefLabel "Throughput"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:performance ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:timeliness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Timeliness measures how up-to-data data is, relative to a specific task."@en ;
  skos:prefLabel "Timeliness"@en ;
  dqv:inCategory qmd:datasetdynamicity ;
  dqv:inCategory qmd:intrinsic ;
.
qmd:timelinessideal
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Ideally, both datasets would be equally fresh, especially with highly volatile data. Differences in freshness could result in inconsistencies even though, at time of publication, each dataset was flawless. This metric measures the difference between the ideal freshness (freshness of the primary dataset) and the freshness of the Target dataset."@en ;
  skos:prefLabel "Timeliness Ideal"@en ;
  dqv:expectedDataType xsd:dayTimeDuration ;
  dqv:inDimension qmd:timeliness ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:timelinessofdatasource
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "This metric measures the delay with which the semantic source is updated in regard to its original source, if present. "@en ;
  skos:prefLabel "Timeliness of Datasource"@en ;
  dqv:expectedDataType xsd:dayTimeDuration ;
  dqv:inDimension qmd:timeliness ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:timelinessofresource
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "A positive difference between current and expiry time of the resource implies data source to be outdated"@en ;
  skos:prefLabel "Timeliness of Resource"@en ;
  dqv:expectedDataType xsd:dayTimeDuration ;
  dqv:inDimension qmd:timeliness ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:timesincelastmodification
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Time since last modification"@en ;
  skos:prefLabel "Time Since Last Modification"@en ;
  dqv:expectedDataType xsd:dayTimeDuration ;
  dqv:inDimension qmd:currency ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:undefinedclasses
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measured usage of undefined classes"@en ;
  skos:prefLabel "Undefined Classes"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:undefinedproperties
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measured usage of undefined properties"@en ;
  skos:prefLabel "Undefined Properties"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:understandability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The ease with which data can be comprehended, without ambiguity, and used."@en ;
  skos:prefLabel "Understandability"@en ;
  dqv:inCategory qmd:representation ;
.
qmd:uniformspatialrepresentation
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Uniformity in datatype for  geometry is measured for this metric. This metric checks whether every geometry is of a specific datatype."@en ;
  skos:prefLabel "Uniform Spatial Representation"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:representationalConsistency ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:validitytimeinterval
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Given that an expiry time is known, a validity interval can be computed based on the time of input and the expiry date. If no expiry time is provided, a measure used in Metric <Exclusion of outdated data> can be used to calculate when the dataset is due for an update. However, this alternative would not measure validity as strictly and instead would measure 'likeliness of being up to date' and hence valid."@en ;
  skos:prefLabel "Validity Time Interval"@en ;
  dqv:expectedDataType xsd:date ;
  dqv:inDimension qmd:volatility ;
  qms:hasMetricType qms:DataQuality ;
.
qmd:verifiability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The ease with which the data can be assessed for correctness."@en ;
  skos:prefLabel "Verifiability"@en ;
  dqv:inCategory qmd:contextual ;
  dqv:inCategory qmd:trust ;
.
qmd:versatility
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The availability of alternative representations and access methods."@en ;
  skos:prefLabel "Versatility"@en ;
  dqv:inCategory qmd:representation ;
.
qmd:vocabularysimilarity
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Interlinking datasets that use different notations for identical information can convolute the process. If both datasets use a vocabulary, it can provide junctions where the two datasets can be interlinked."@en ;
  skos:prefLabel "Vocabulary Similarity"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:representationalConsistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:voidvalues
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Usage owl:Inverse-functionalProperty implies a resource can be uniquely identified by the value of the property. When this usage results in faulty values (void values), it will no longer uniquely identify a resource, but each resource with a similar void value, resulting in an explosion of owl:sameAs links."@en ;
  skos:note "void values should be listed and respective instances annotated."@en ;
  skos:prefLabel "Void Values"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:volatility
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The frequency of changes in the data over time."@en ;
  skos:prefLabel "Volatility"@en ;
  dqv:inCategory qmd:datasetdynamicity ;
.
