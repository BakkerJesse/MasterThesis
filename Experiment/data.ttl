# baseURI: http://data.labs.pdok.nl/quality/id/
# imports: http://data.labs.pdok.nl/quality/def
# imports: http://data.labs.pdok.nl/quality/req/cbs
# prefix: qmd

@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qmd: <http://data.labs.pdok.nl/quality/id/> .
@prefix qms: <http://data.labs.pdok.nl/quality/def#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix reqCBS: <http://data.labs.pdok.nl/quality/req/cbs#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

qmd:
  rdf:type owl:Ontology ;
  spin:imports <http://topbraid.org/spin/owlrl-all> ;
  owl:imports <https://data.labs.pdok.nl/quality/def> ;
  owl:imports <https://data.labs.pdok.nl/quality/req/cbs> ;
.
qmd:QualityMeasurementDataset
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:QualityMeasurementDataset ;
  rdf:type void:Dataset;
  dcterms:created "2017-06-17"^^xsd:date ;
  dcterms:description "A dataset containing quality measurements about CBS/2015 with requirements based on BAG"@en ;
  dcterms:identifier qms:QualityMeasurementDataset ;
  dcterms:title "Quality Measurement Dataset"@en ;
  skos:prefLabel "Quality Measurement Dataset"@en ;
  qms:hasPrimaryDataset <https://bag.basisregistraties.overheid.nl/> ;
  qms:hasTargetDataset <https://data.pdok.nl/cbs/2015/buurt/> ;
.
qmd:RDFdumpavailability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Checking whether accessible/downloadable RDF dumps are provided. "@en ;
  skos:prefLabel "RDF Dump Availability"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:availability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:SPARQLendpointaccessibility
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Checking whether a working SPARQL endpoint is present. No differentiation is made based on the quality of the response for this measure (either based on time or content)."@en ;
  skos:prefLabel "SPARQL Endpoint Accessibility"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:availability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:URIlength
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "URIs should be readable and concise. Making sure there is no redundant information is present in the URI makes it more easily understood and de-referenced."@en ;
  skos:prefLabel "URIs Length"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:representationalConciseness ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:accuracy
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Degree of correctness and precision with which information in an information system represents states of the real world. Can be divided into semantic (correctness in regard to real world values) and syntactic (correctness in regard to its definition domain) accuracy."@en ;
  skos:prefLabel "Accuracy"@en ;
  dqv:inCategory qmd:intrinsic ;
.
qmd:ambiguousinstances
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Instances mapping to more than one real-world object, resulting in multiple possible interpretations."@en ;
  skos:prefLabel "Ambiguous Instances"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:amountOfData
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Measures the appropriateness of the volume and quantity of data for a given task. "@en ;
  skos:prefLabel "Amount of Data"@en ;
  dqv:inCategory qmd:contextual ;
.
qmd:amountofclasses
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "The amount of distinct classes in the dataset."@en ;
  skos:prefLabel "Amount of Classes"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:amountofproperties
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "The amount of distinct properties in the dataset."@en ;
  skos:prefLabel "Amount of Properties"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:amountofresources
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "The amount of resources in the dataset."@en ;
  skos:prefLabel "Amount of Resources"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:DataQuality ;
.
qmd:amountoftriples
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "The amount of triples in the dataset."@en ;
  skos:prefLabel "Amount of Triples"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:attributeAccuracy
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Degree in which measurements are correct, or how precise the measurement was conducted. For instance with Geo spatial data, measurements can be done in meters or millimeters. "@en ;
  skos:prefLabel "Attribute Accuracy"@en ;
  dqv:expectedDataType rdfs:Literal ;
  dqv:inDimension qmd:accuracy ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:availability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Availability of a dataset is the extent to which data (or some portion of it) is present, obtainable and ready for use."@en ;
  skos:prefLabel "Availability"@en ;
  dqv:inCategory qmd:accessibility ;
.
qmd:believability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The degree to which the data is considered true and correct."@en ;
  skos:prefLabel "Believability"@en ;
  dqv:inCategory qmd:contextual ;
  dqv:inCategory qmd:trust ;
.
qmd:blanknodes
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measures the percentage of blank nodes among the total number of URIs. "@en ;
  skos:prefLabel "Blank Nodes"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:interpretability ;
  qms:hasMetricType qms:DataQuality ;
.
qmd:community
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Providing message boards and mailing list can be used to actively improve, and help users understand and query the dataset. This metric measure the presence and use of such mechanism. A neglected message board might indicate that the dataset is no longer updated. The value of this metric is True only when message boards and/or mailing list are provided and used."@en ;
  skos:prefLabel "Community"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:understandability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:completeness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The degree to which all required information is present in a dataset. completeness can be determined on three levels, 1) schema, also ontology completeness, 2) property completeness, measure of missing values and 3) population completeness, percentage of real world objects represented in the dataset."@en ;
  skos:prefLabel "Completeness"@en ;
  dqv:inCategory qmd:contextual ;
.
qmd:conciseness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Conciseness concerns the presence of redundant information in the dataset, either at schema level or instance level."@en ;
  skos:prefLabel "Conciseness"@en ;
  dqv:inCategory qmd:intrinsic ;
.
qmd:consistency
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "A dataset is consistent if it is free of conflicting information with respect to particular knowledge representation and inference mechanisms."@en ;
  skos:prefLabel "Concistency"@en ;
  dqv:inCategory qmd:intrinsic ;
.
qmd:contentnegotiation
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Checking whether data can be retrieved in accepted formats and languages by adding a corresponding accept-header to an HTTP request"@en ;
  skos:prefLabel "Content Negotiation"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:versatility ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:contenttype
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Meta data attached to the http request when dereferencing a URI in a browser, can include a 'content type'. User agents can use this to quickly identify pages containing RDF. "@en ;
  skos:prefLabel "Content Type"@en ;
  dqv:expectedDataType xsd:string ;
  dqv:inDimension qmd:availability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:copyright
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detection of whether the work is attributed in the same way as specified by the author or licensor. Share-alike and copyleft roughly imply that the work in question should be licensed similarly as referenced sources. The work in question can also specify share-alike and copyleft requirements."@en ;
  skos:prefLabel "Copyright"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:licencing ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:coverage
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Coverage measures the scope of the dataset as the number of entities represented and the descriptiveness as the number of properties known for each entity."@en ;
  skos:prefLabel "Coverage"@en ;
  dqv:inDimension qmd:amountOfData ;
  qms:hasMetricType qms:InstanceQuality ;
  qms:hasPart qmd:descriptiveness ;
  qms:hasPart qmd:scope ;
.
qmd:currency
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Measures the speed with with data values are updates after the real-world values change."@en ;
  skos:prefLabel "Currency"@en ;
  dqv:inCategory qmd:datasetdynamicity ;
.
qmd:currencyofdataset
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measures how current the data is."@en ;
  skos:prefLabel "Currency of Dataset"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:currency ;
  qms:hasMetricType qms:DataQuality ;
.
qmd:currencyofdatasetcomparison
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measure the difference between the primary and Target dataset in terms of currency. "@en ;
  skos:prefLabel "Currency of Dataset Comparison"@en ;
  dqv:expectedDataType xsd:dayTimeDuration ;
  dqv:inDimension qmd:currency ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:dataaccesssecurity
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Use of login credentials or use of SSL or SSH."@en ;
  skos:prefLabel "Data Access Security"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:security ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:dataofproprietarynature
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Data only available to specific users is of proprietary nature. Even if only parts of the data is hidden from access for the outside world (possibly due to a sensitive nature of the data), this metric should be scored 'False'"@en ;
  skos:prefLabel "Data of Proprietary Nature"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:security ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:datasetlanguage
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Checking whether data is available in different languages. For some purposes one would require the Target dataset to be in the same language as the primary dataset or at least another language that is sufficiently understood by the target users. This metric measures the amount of languages the dataset is published in. "@en ;
  skos:prefLabel "Dataset Language"@en ;
  dqv:expectedDataType xsd:language ;
  dqv:inDimension qmd:versatility ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:de-referencability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "When a URI can not be properly dereferenced through a web browser and thus returns a server or client  error, this is seen as a de-referencability issue."@en ;
  skos:note "Each URI that turned out to be not dereferencable, should be annotated. The proposed method is by means of the triple: <subject> qmd:isDereferencable 'False' "@en ;
  skos:prefLabel "De-referencability"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:availability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:de-referencedbacklinks
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "detection of all local in-links or back-links: locally available triples in which the resource URI appears as an object, in the dereferenced document returned for the given resource."@en ;
  skos:prefLabel "De-referenced Back-Links"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:availability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:de-referencedforwardlinks
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detection of all forward links: locally known triples where the local URI is mentioned in the subject"@en ;
  skos:prefLabel "De-referenced Forward-Links"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:availability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:deprecatedclasses
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Count the occurences of classes of type owl:DeprecatedClass"@en ;
  skos:prefLabel "Deprecated Classes"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:deprecatedproperties
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Count the occurences of classes of type owl:DeprecatedProperty"@en ;
  skos:prefLabel "Deprecated Properties"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:descriptiveness
  rdf:type owl:NamedIndividual ;
  skos:definition "Measure the average number of attributes known for entities"@en ;
  skos:prefLabel "Descriptiveness"@en ;
  dqv:expectedDataType xsd:float ;
  qmd:isPartOf qmd:coverage ;
.
qmd:disjointclasses
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "entities (inferred to be) part of two disjoint classes. This metric measures the percentage of entities where this is the case."@en ;
  skos:prefLabel "Disjoint Classes"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:duplicateinstance
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measures the amount of duplicate instances"@en ;
  skos:prefLabel "Duplicate Instance"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:conciseness ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:exemplarySPARQL
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "As with the exemplary URIs, exemplary SPARQL queries provide insight as to how the dataset can be queried. "@en ;
  skos:prefLabel "Exemplary SPARQL"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:understandability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:exemplaryURIs
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detecting whether the URI patterns are provided"@en ;
  skos:prefLabel "Exemplary URIs"@en ;
  dqv:expectedDataType xsd: ;
  dqv:inDimension qmd:understandability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:extensionalconciseness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measures the percentage of unique objects in the dataset. "@en ;
  skos:prefLabel "Extensional Conciseness"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:conciseness ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:frequencyofchange
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Refer to the changefrequency attribute in a Semantic Sitemap for value of the frequency or updates of a data source"@en ;
  skos:note "expected datatype range is limited to the following: [always, hourly, daily, weekly, monthly, yearly, never]"@en ;
  skos:prefLabel "Frequency of Change"@en ;
  dqv:expectedDataType xsd:string ;
  dqv:inDimension qmd:volatility ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:geometryblanknode
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "The dataset contains instances representing all the 'buurten' (neighbourhoods) in the Netherlands. Each of these should be accompanied by a geometry representing the geographical area considered a specific neighbourhood. Best practices mandate that a geometry should be attached to a blank node, which is in turn linked to the neighbourhood."@en ;
  skos:prefLabel "Geometry Blank Node"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:Interpretability ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:human-readablelabelling
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "percentage of resources containing a human readable label. For instance by means of an rdfs:label or rdfs:comment. Other vocabularies also offer labelling functions such as skos:preflabel or skos:note."@en ;
  skos:prefLabel "Human-Readable Labelling"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:understandability ;
  qms:hasMetricType qms:DataQuality ;
.
qmd:human-readablelicence
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detection of a license in the documentation of the dataset or its source"@en ;
  skos:prefLabel "Human-Readable Licence"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:licencing ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:inaccuratefacts
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Check for inconsistencies by comparing the data to that of other sources. The metric measures the amount of facts deemed correct, by lack of evidence against the fact."@en ;
  skos:prefLabel "Inaccurate Facts"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:accuracy ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:inaccuratevalues
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "This metric measures the amount of inaccurate values by using functional dependencies rules between the values of two or more different predicates. A functional dependency is a constraint between different properties of an instance, in the case of linked data. the underlying concept is that if one value for property X of an instance always co-occurs with a precisely one value for property Y, then it can be said that X determines the value of Y. "@en ;
  skos:prefLabel "Inaccurate Values"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:accuracy ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:incompatibledatatyperange
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detection of ill-typed literals which do not abide by the lexical syntax for their respective datatype."@en ;
  skos:prefLabel "Incompatible Datatype Range"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:accuracy ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:indicativeness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Denotes the amount of metrics measured in the form of a grade between 1 and 10."@en ;
  skos:prefLabel "Indicativeness"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:verifiability ;
  qms:hasMetricType qms:Quality ;
.
qmd:instanceenrichment
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Percentage of attributes gained by interlinking and owl:sameAs relations."@en ;
  skos:prefLabel "Instance Enrichment"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:interlinking ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:intentionalconciseness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measures the percentage of unique attributes in the dataset. "@en ;
  skos:prefLabel "Intentional Conciseness"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:consiceness ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:interlinking
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Interlinking involves creating triples where the object and subject refer to the same concept and the object and subject can either originate from different datasets, of the same dataset. The interlinking dimension refers to the extend where this the case."@en ;
  skos:prefLabel "Interlinking"@en ;
  dqv:inCategory qmd:accessibility ;
.
qmd:interlinkingcompleteness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Interlinking completeness measures the amount of instances that are interlinked in respect to the total amount of instances."@en ;
  skos:note "Metric can only be measured after interlinking."@en ;
  skos:prefLabel "Interlinking Completeness"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:interpretability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Interpretability covers a more technical aspect of information. It concerns the usage of appropriate languages, symbols and units. Interpretability can be enhanced by offering data conform best practices. It should also be noted that the more interpretable the data is, the easier it is to integrate it with other datasets."@en ;
  skos:prefLabel "Interpretability"@en ;
  dqv:inCategory qmd:representation ;
.
qmd:interpretabilityofdata
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detect the use of appropriate language, symbols, units and clear definitions. For instance, the use of abbreviations can make the dataset highly understandable, but its interpretation is limited to those who know the abbreviations."@en ;
  skos:prefLabel "Interpretability of Data"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:interpretability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:interpretabilityofterms
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Use of various schema languages to provide definitions for terms. Providing definitions for used terms helps users to interpret the information. "@en ;
  skos:prefLabel "Interpretability of Terms"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:interpretability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:latency
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Latency is the delay between the submission of a request by a user, and the reception of a response from the system."@en ;
  skos:note "Unit of measure is milliseconds (ms)"@en ;
  skos:prefLabel "Latency"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:performance ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:licencing
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Concerns under which terms data can be (re)used."@en ;
  skos:prefLabel "Licencing"@en ;
  dqv:inCategory qmd:accessibility ;
