  qms:hasMetricType qms:DatasetQuality ;
.
qmd:populationcompleteness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "This metric measures the percentage of real-world objects represented in the dataset. Attaching a number to the amount of real-world object is a difficult task. In some cases it can be assumed that the primary dataset, approximately, contains the total amount of real-world objects. in this case the total amount of objects in the measure function is known. In other cases this number can be obtained by consulting other trusted data sources."@en ;
  skos:prefLabel "Population Completeness"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:prefixindication
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "With the dataset, an indicaion of these external vocabularies should be provided to enhance understanding of the dataset. "@en ;
  skos:prefLabel "Prefix Indication"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:understandability ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:prolixRDFfeatures
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measure the usage of too lengthy rdf features such rdf reification, rdf containers and rdf collections. it is advised not to use these since it make the use of SPARQL more cumbersome and since the semantics behind these features are unclear."@en ;
  skos:prefLabel "Prolix RDF Features"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:representationalConciseness ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:propertycompleteness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Property completeness entails every entity having the properties described in the schema known."@en ;
  skos:prefLabel "Property Completeness"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:InstanceQuality ;
.
qmd:propertyconsistency
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Measures the number of instances with identical values for different attributes. When two or more properties of an instance contains the same information, but with different naming, it could be redundant. "@en ;
  skos:prefLabel "Property Consistency"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:propertytype
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Properties of a resource can refer to either a literal value, or to a URI/blank-node, an owl:DatatypeProperty and a n owl:ObjectProperty, respectively. This metric measures the amount of triples where the datatype of the subject is incompatible with the predicate (property)."@en ;
  skos:prefLabel "Property Type"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:consistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:re-usedvocabularies
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "The usage of established vocabularies boosts the understandability (and uniformity) of the data schemas and thereby the ease of reuse. This metric measures the number of external vocabularies used."@en ;
  skos:prefLabel "Re-used Vocabularies"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:representationalConsistency ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:relevancy
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The extend to which information is applicable and helpful for a given task, and more specifically a query."@en ;
  skos:prefLabel "Relevancy"@en ;
  dqv:inCategory qmd:contextual ;
.
qmd:representationalConciseness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "The extent to which the data is compact, clearly formatted and complete."@en ;
  skos:prefLabel "Representational Conciseness"@en ;
  dqv:inCategory qmd:representation ;
.
qmd:representationalConsistency
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Representational-consistency is the degree to which the format and structure of the information conforms to previously returned information as well as data from other sources."@en ;
  skos:prefLabel "Representational Consistency"@en ;
  dqv:inCategory qmd:representation ;
.
qmd:reputation
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Dimension ;
  skos:definition "Perceived trustworthiness of a dataset."@en ;
  skos:prefLabel "Reputation"@en ;
  dqv:inCategory qmd:contextual ;
  dqv:inCategory qmd:trust ;
.
qmd:reputationofdataset
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "By assigning explicit ratings to the dataset (manual) and analyzing external links or page rank (semi-automated)"@en ;
  skos:prefLabel "Reputation of Dataset"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:reputation ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:resultsizerestriction
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Time-out errors can occur when the SPARQL query attempts to retrieve large amounts of data. Data providers can impose such restriction as well. The value for this metric is zero if no restriction is imposed."@en ;
  skos:note "The value of this metric is the max amount of results returned for a single query. If no restrictions are posed, the value is zero."@en ;
  skos:prefLabel "Result Size Restriction"@en ;
  dqv:expectedDataType xsd:integer ;
  dqv:inDimension qmd:performance ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:scalability
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Detection of whether the time to answer an amount of ten requests divided by ten is not longer than the time it takes to answer one request."@en ;
  skos:prefLabel "Scalability"@en ;
  dqv:expectedDataType xsd:boolean ;
  dqv:inDimension qmd:performance ;
  qms:hasMetricType qms:DatasetQuality ;
.
qmd:schemacompleteness
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Usage of external (and established) vocabularies improves the machine-readability and reusability of a schema. They also provide an overview of properties and classes relevant for a given domain and scope. This metric measures the extend to which the vocabulary is exploited by taking the percentage of distinct classes and properties of an external vocabulary reused in the dataset.   "@en ;
  skos:prefLabel "Schema Completeness"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:completeness ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:schemaenrichment
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "Percentage of classes and properties reused from other sources."@en ;
  skos:prefLabel "Schema Enrichment"@en ;
  dqv:expectedDataType xsd:float ;
  dqv:inDimension qmd:interlinking ;
  qms:flags rdf:type ;
  qms:flags <http://www.opengis.net/ont/geosparql#Geometry> ;
  qms:flags <http://www.opengis.net/ont/geosparql#asWKT> ;
  qms:flags <http://www.opengis.net/ont/geosparql#hasGeometry> ;
  qms:hasMetricType qms:SchemaQuality ;
.
qmd:schemasimilarity
  rdf:type owl:NamedIndividual ;
  rdf:type dqv:Metric ;
  skos:definition "no. of classes and properties represented / total no. of classes and properties. for the purpose of interlinking, a more interesting form would take into account the intersection of the schema of both datasets as well as the parts only present in one of the datasets and their respective size."@en ;
  skos:note "Schema similarity metric annotates the number of classes and properties unique to each dataset and classes and properties shared by the datasets. The expected data type for this metrics depicts a 'similarity score' based on the previously mentioned amounts. "@en ;
